{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "css/*.css", "use": "@vercel/static" },
    { "src": "js/*.js", "use": "@vercel/static" }
  ],
  "env": {
    "BOT_TOKEN": "@adsclickpay_bot_token",
    "BOT_CHAT_ID": "@adsclickpay_chat_id",
    "BOT_USERNAME": "@adsclickpay_bot_username"
  }
}
